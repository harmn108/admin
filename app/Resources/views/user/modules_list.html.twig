{% if modules is defined %}
    {% for module in modules %}
        <tr class="text-center" data-id="{{ module.id }}">
            <td>{{ module.id }}</td>
            <td>
                <a  class="module_route" href="{{ module.route }}">{{ module.name }}</a>
            </td>
            <td>
                <i class="fa fa-pencil-square-o pointer update_module" aria-hidden="true" title="Edit Module"></i>
            </td>
        </tr>
    {% endfor %}
{% endif %}