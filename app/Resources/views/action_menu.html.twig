{% set key = app.request.cookies.get('X-TOKEN') %}
{% set userInfo = app.session.get(key) %}

<nav id="action_nav" class="navbar navbar-default">
    <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
            {% if userInfo['pages'] is defined %}
                {% for page in userInfo['pages'] %}
                    <li><a href="#">{{ page }}</a></li>
                {% endfor %}
            {% endif %}

        </ul>
    </div>
</nav>